# Gets emoji from the associative array
# Returns raw emoji character or word
to_emoji () {
    local word="$1"
    local emoji_name="$word"
    regexp-replace emoji_name '[^a-zA-Z0-9_]' ''
    local emoji_value=${emojis[${emoji_name:l}]}
    if [[ -z $emoji_value ]]; then
        echo "$word"
        return
    fi
    regexp-replace word ":$emoji_name:" $emoji_value
    echo $word
}

# Function to parse a line, split it into an array of words and then emojify
# each word.
parse_line () {
    OLD_IFS=$IFS;
    IFS=' ';
    local words="$@"
    local words_arr=(${=words})
    out=()
    for word in "${words_arr[@]}"; do
        out+=("$(to_emoji "$word")")
    done
    echo "${(j. .)out}"
    IFS=$OLD_IFS
}

# Function to print help info.
help () {
    cat <<EOF
    ${scriptname} - emoji on the command line 😱

    USAGE: ${scriptname} [-h|--help] [-l|--list] TEXT
EOF
    if [[ 'list' == $1 ]]; then
        echo 'Supported emojis:'
        for key in ${(nk)emojis}; do
            echo -e ":$key: ${emojis[$key]}"
        done
    fi
    exit 0
}

(( ! $+functions[regexp-replace] )) && autoload -U regexp-replace
if [[ -n $1 ]]; then
    case $1 in
    '-h' | '--help' )
        help
        ;;

    '-l' | '--list' )
        help list
        ;;

    * )
        parse_line "$*"
        ;;
    esac
else
    while IFS=''; read -r line || [ -n "$line" ]; do
        parse_line "$line"
    done
fi
